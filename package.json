{
  "name": "ooo-admin-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "OOO Admin Boilerplate",
  "license": "MIT",
  "main": "index.tsx",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:us": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "develop": "cross-env CUSTOM_ENV=staging webpack-dev-server --env.CUSTOM_ENV=$CUSTOM_ENV",
    "prod": "cross-env CUSTOM_ENV=production webpack-dev-server --env.prod=true",
    "build": "rimraf build && webpack --env.prod=true --env.sw=true --env.CUSTOM_ENV=$CUSTOM_ENV",
    "start": "pushstate-server -d build/ -p 8080",
    "perf": "rimraf *.report.dom.html && npm run build && (npm run serve & lighthouse http://localhost:4000 --view)",
    "lint": "eslint ./app -f table",
    "lint:fix": "eslint ./app -f table --fix",
    "update-types": "flow-typed install",
    "flow": "flow",
    "postinstall": "rimraf .cache-loader",
    "deployProd": "gcloud app deploy ./app-production.yaml --project gothic-depth-196420",
    "deployStaging": "gcloud app deploy ./app-staging.yaml --project gothic-depth-196420"
  },
